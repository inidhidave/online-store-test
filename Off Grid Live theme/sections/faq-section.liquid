{% schema %}
  {
    "name": "Faq Section",
    "settings": [
		{
			"type": "text",
			"id": "title",
			"label": "Heading"
		}
	],
	"blocks": [
	 {
	   "type": "block",
	   "name": "Faq Block",
	   "settings": [
	   {
		 "type": "text",
		 "id": "faq_heading",
		 "label": "Faq Heading"
	   },
	   {
		 "type": "textarea",
		 "id": "faq_desc",
		 "label": "Faq Description"
	   }
	]
 }
]
  }
{% endschema %}

