<div class="container">
 <h2>{{ section.settings.title }}</h2>
 <div class="row">
  <div class="col-md-8 offset-md-2">
<div class="bd-example" data-example-id="">
<div id="accordion" role="tablist" aria-multiselectable="true">
  
{%- assign contentIndex = 0 -%}
 {% for block in section.blocks %}  
  <div class="card">
    <div class="card-header" role="tab" id="heading{{ contentIndex }}">
      <div class="mb-0">
        <a data-toggle="collapse" data-parent="#accordion" href="#collapse{{ contentIndex }}" aria-expanded="false" aria-controls="{{ contentIndex }}" class="collapsed">
            <h3>{{ block.settings.faq_heading }}</h3>
            <i class="plus-icon" aria-hidden="true"></i>
        </a>
      
      </div>
    </div>

    <div id="collapse{{ contentIndex }}" class="collapse" role="tabpanel" aria-labelledby="heading{{ contentIndex }}" aria-expanded="false" style="">
      <div class="card-block">
        {{ block.settings.faq_desc }} 
      </div>
    </div>
  </div>
  {%- assign contentIndex = contentIndex | plus: 1 -%}
  {% endfor %}
